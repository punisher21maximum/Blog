Read space separated N input integers from terminal

	    int N;
      scanf("%d", &N);
	    int arr[N]; char ch;
	    for(j=0; j<N; j++)
	    {
	        scanf("%d%c", &arr[j], ch);
	    }
	    for(j=0; j<N; j++)
	    {
	        printf("%d ", arr[j]);
	    }
	    
	    
